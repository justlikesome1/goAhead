<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库链接地址账号密码-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/alltest?characterEncoding=utf8" userId="root" password="123456">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!-- 封装类 -->
        <javaModelGenerator targetPackage="com.woniu.pojo"
                            targetProject="SSMFirst">
            <property name="enableSubPackages" value="false" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
		<!-- mapper.xml -->
        <sqlMapGenerator targetPackage="com.woniu.mapper"
                         targetProject="SSMFirst">
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>
		<!-- mapper接口 -->
        <javaClientGenerator targetPackage="com.woniu.mapper"
                             targetProject="SSMFirst"
                             type="XMLMAPPER">
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>
        <!--生成对应表及类名-->
        <table tableName="dispath" domainObjectName="Dispath" schema="alltest" ></table>
        <table tableName="car" domainObjectName="Car"  schema="alltest"></table>
        <table tableName="driver" domainObjectName="Driver"  schema="alltest"></table>
        <table tableName="autocade" domainObjectName="Autocade"  schema="alltest"></table>
        <table tableName="dept" domainObjectName="Dept"  schema="alltest"></table>
        <table tableName="emp" domainObjectName="Emp"  schema="alltest"></table>
        <table tableName="user" domainObjectName="User"  schema="alltest"></table>
    </context>
</generatorConfiguration>